<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle"
            >

            <stroke
                android:width="1dp"
                android:color="@color/colorPrimary"
                android:dashWidth="@dimen/_5sdp"
                android:dashGap="@dimen/_5sdp" />

            <size
                android:width="@dimen/_65sdp"
                android:height="@dimen/_65sdp" />

            <corners
                android:bottomLeftRadius="@dimen/_10sdp"
                android:bottomRightRadius="@dimen/_10sdp"
                android:topLeftRadius="@dimen/_10sdp"
                android:topRightRadius="@dimen/_10sdp" />
        </shape>

    </item>
</layer-list>